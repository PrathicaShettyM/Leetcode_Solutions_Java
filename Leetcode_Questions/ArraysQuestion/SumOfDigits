class Solution {
    public int sumOfDigits(int n){
        int sum = 0;
        while(n!=0){
            int lastDigit = n%10;
            sum += lastDigit;
            n /= 10;
        }
        return sum;
    }
    public int countEven(int num) {
        int count = 0;
        for(int i = 1; i <= num; i++){
            if(sumOfDigits(i)%2==0) count+=1;
        }
        return count;
    }
}